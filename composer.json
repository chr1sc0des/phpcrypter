{
    "name": "chrisvpearse/phpcrypter",
    "description": "A PHP source code encrypter.",
    "keywords": [
        "chrisvpearse",
        "phpcrypter"
    ],
    "homepage": "https://github.com/chrisvpearse/phpcrypter",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Pearse",
            "email": "chris@cpearse.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-openssl": "*",
        "symfony/console": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Crypter\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/phpcrypter"
    ]
}
